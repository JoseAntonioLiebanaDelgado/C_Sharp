<Window x:Name="RegisterWindow" x:Class="AccessControl.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Register" Height="350" Width="525">
    <Grid>
        <TextBox Name="UserName" VerticalContentAlignment="Center" HorizontalAlignment="Left" Height="30" TextWrapping="Wrap" Text="UserName" VerticalAlignment="Top" Width="342" Margin="87,82,0,0" GotFocus="UserName_GotFocus" />
        <!--TextBox Name="Password" HorizontalAlignment="Left" Height="30" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="342" Margin="87,127,0,0" GotFocus="Password_GotFocus" />-->
        <Button Name="RegisterButton" Content="Register" HorizontalAlignment="Left" Height="30" Margin="87,175,0,0" VerticalAlignment="Top" Width="342" Click="RegisterButton_Click"/>
        <PasswordBox Name="Password"  VerticalContentAlignment="Center" HorizontalAlignment="Left" Height="30" Margin="87,123,0,0" VerticalAlignment="Top" Width="342"/>


    </Grid>
</Window>
